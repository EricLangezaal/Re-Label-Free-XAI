#!/bin/bash

#SBATCH --partition=gpu_titanrtx_shared_course
#SBATCH --gres=gpu:1
#SBATCH --job-name=sprites
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=3
#SBATCH --time=08:00:00
#SBATCH --mem=32000M
#SBATCH --output=sprites_%A.out

module purge
module load 2021
module load Anaconda3/2021.05

# activate the environment
source activate fact

now=$(date +"%T")
echo "Running sprites on MNIST time is $now"
python -m dsprites
now=$(date +"%T")
echo "Finished sprites on MNIST time is $now"